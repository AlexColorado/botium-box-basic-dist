{
  "name": "botium-box-basic-dist",
  "scripts": {
    "start:agent": "cd agent && npm run run:dist",
    "start:server": "cd server && npm run run:dist",
    "start": "npm-run-all -p start:agent start:server",
    "prepare:agent": "cd agent && npm install --production",
    "prepare:server": "cd server && npm install --production && npm run prisma-deploy && npm run prisma-seed",
    "prepare": "npm-run-all prepare:agent prepare:server",
    "postinstall": "npm run prepare"
  },
  "dependencies": {
    "npm-run-all": "^4.1.3"
  }
}