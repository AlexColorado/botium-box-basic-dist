{
  "name": "botium-box-server",
  "scripts": {
    "start": "nodemon -e js,graphql -x node -r dotenv/config src/index.js",
    "debug": "nodemon -e js,graphql -x node --inspect -r dotenv/config src/index.js",
    "playground": "graphql playground",
    "dev": "npm-run-all --parallel start playground",
    "prisma-deploy": "prisma deploy",
    "link": "npm link botium-core",
    "start:prod": "node -r dotenv/config src/index.js dotenv_config_path=.env.production",
    "prisma-deploy:prod": "prisma deploy -e .env.production",
    "eslint": "eslint \"./src/**/*.js\"",
    "build:dist": "NODE_ENV=production rollup -c && javascript-obfuscator dist/botium-box-server.js --output dist/botium-box-server.js --compact true --self-defending true",
    "run:dist": "NODE_ENV=production node -r dotenv/config  dist/botium-box-server.js dotenv_config_path=.env.production",
    "publish:free-dist": "npm run build:dist && mkdir -p ~/botium-box-basic-dist/server/src/generated && cp -Rf dist .env.production package.json package-lock.json src/schema.graphql ~/botium-box-basic-dist/server && cp src/schema.graphql ~/botium-box-basic-dist/server/src && cp src/generated/prisma.graphql ~/botium-box-basic-dist/server/src/generated"
  },
  "dependencies": {
    "async": "^2.6.1",
    "bcryptjs": "^2.4.3",
    "botium-connector-alexa-smapi": "0.0.2",
    "botium-connector-dialogflow": "0.0.4",
    "botium-connector-directline3": "0.0.5",
    "botium-connector-watson": "0.0.1",
    "botium-connector-webdriverio": "0.0.6",
    "botium-core": "^1.3.29",
    "connect-history-api-fallback": "^1.5.0",
    "debug": "^4.0.1",
    "dotenv": "^6.1.0",
    "graphql-yoga": "^1.16.2",
    "jsonwebtoken": "^8.3.0",
    "kue": "^0.11.6",
    "moment": "^2.22.2",
    "node-cache": "^4.2.0",
    "prisma-binding": "^2.1.5",
    "randomatic": "^3.1.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "slugify": "^1.3.2",
    "uuid": "^3.3.2",
    "watson-developer-cloud": "^3.11.1"
  },
  "devDependencies": {
    "eslint": "^5.8.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "graphql-cli": "^2.16.7",
    "javascript-obfuscator": "^0.18.1",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "prisma": "^1.16.2",
    "rollup": "^0.67.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-json": "^3.1.0"
  }
}
